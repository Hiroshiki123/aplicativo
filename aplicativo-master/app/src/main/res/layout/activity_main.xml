<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="177dp"
        android:layout_marginTop="145dp"
        android:layout_marginEnd="177dp"
        android:layout_marginBottom="567dp"
        android:fontFamily="@font/bungee"
        android:text="JCV"
        android:textSize="96sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="158dp"
        android:layout_marginTop="279dp"
        android:layout_marginEnd="159dp"
        android:text="Button"
        app:layout_constraintBottom_toTopOf="@+id/toggleButton3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <com.google.android.material.chip.ChipGroup
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="82dp"
        tools:layout_editor_absoluteY="627dp">

    </com.google.android.material.chip.ChipGroup>

    <ToggleButton
        android:id="@+id/toggleButton"
        android:layout_width="56dp"
        android:layout_height="47dp"
        android:layout_marginStart="138dp"
        android:layout_marginTop="612dp"
        android:layout_marginEnd="38dp"
        android:text="ToggleButton"
        app:layout_constraintBottom_toTopOf="@+id/textView2"
        app:layout_constraintEnd_toStartOf="@+id/toggleButton3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ToggleButton
        android:id="@+id/toggleButton3"
        android:layout_width="56dp"
        android:layout_height="47dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="65dp"
        android:layout_marginEnd="139dp"
        android:text="ToggleButton"
        app:layout_constraintBottom_toTopOf="@+id/textView3"
        app:layout_constraintEnd_toStartOf="@+id/toggleButton4"
        app:layout_constraintStart_toEndOf="@+id/toggleButton"
        app:layout_constraintTop_toBottomOf="@+id/button" />

    <ToggleButton
        android:id="@+id/toggleButton4"
        android:layout_width="56dp"
        android:layout_height="47dp"
        android:text="ToggleButton"
        tools:layout_editor_absoluteX="294dp"
        tools:layout_editor_absoluteY="611dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="68dp"
        android:layout_height="21dp"
        android:layout_marginStart="51dp"
        android:layout_marginEnd="31dp"
        android:layout_marginBottom="51dp"
        android:text="VIBRAÇÂO"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/textView3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toggleButton" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="90dp"
        android:layout_height="21dp"
        android:layout_marginStart="160dp"
        android:layout_marginEnd="46dp"
        android:layout_marginBottom="53dp"
        android:text="QUALID. ALTA"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/textView4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toggleButton3"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="53dp"
        android:layout_height="21dp"
        android:layout_marginStart="233dp"
        android:layout_marginBottom="52dp"
        android:text="MÚSICA"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toggleButton4" />

</androidx.constraintlayout.widget.ConstraintLayout>